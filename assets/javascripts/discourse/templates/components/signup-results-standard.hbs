{{#each option in options}}
  <li>
    <div class="option">
      <p>
        <span class="percentage">{{option.percentage}}%</span>
        <strong>{{{option.html}}}</strong>
      </p>
    </div>
    <div class="bar-back">
      <div class="bar" style={{option.style}}></div>
    </div>

    {{signup-results-standard-voters option=option}}
  </li>
{{/each}}
